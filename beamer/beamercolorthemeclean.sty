\definecolor{greyone}{RGB}{77,77,77}
\definecolor{greytwo}{RGB}{150,150,150}
\definecolor{alertred}{RGB}{200,75,75}

% Header areas
\setbeamercolor{palette quaternary}{fg=white,bg=greyone}
\setbeamercolor{titlelike}{parent=palette quaternary}

% Verbatim font color
\renewcommand\verbatim@font{\color{white}\normalfont\ttfamily}

% Itemization and enumeration color
\setbeamercolor{itemize item}{fg=greyone}
\setbeamercolor{itemize subitem}{fg=greyone}
\setbeamercolor{itemize subsubitem}{fg=greyone}
\setbeamercolor{enumerate item}{fg=greyone}
\setbeamercolor{enumerate subitem}{fg=greyone}
\setbeamercolor{enumerate subsubitem}{fg=greyone}

% Table of contents
\setbeamercolor{section in toc}{fg=black}
\setbeamercolor{subsection in toc}{fg=greytwo}

% Various
\setbeamercolor{alerted text}{fg=alertred}